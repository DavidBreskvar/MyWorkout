<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dbug.myworkout.app"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="dbug.myworkout.app.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="dbug.myworkout.app.SavedWorkout"
            android:label="@string/title_activity_saved_workout" >
        </activity>
        <activity
            android:name="dbug.myworkout.app.CelebrityWorkout"
            android:label="@string/title_activity_celebrity_workout" >
        </activity>
        <activity android:name="BaseActivity">
        </activity>
        <activity 
            android:name="NewWorkout"
            android:label="New Workout">
        </activity>
        <activity 
            android:name="NewWorkoutActivity"
            android:label="New Workout Activity"></activity>
    </application>

</manifest>
